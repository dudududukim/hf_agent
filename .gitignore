cat > .gitignore << 'EOF'
# Environment variables
.env
.env.local
.env.*.local

# Python version
.python-version

# Virtual environments
.venv
venv/
ENV/
env/

# Python cache
__pycache__/
*.py[cod]
*$py.class

# Distribution / packaging
build/
dist/
*.egg-info/
.eggs/
*.egg

# Jupyter Notebook
.ipynb_checkpoints
*.ipynb_checkpoints/

# Database
cv_chroma_db/
chroma_db/
*.db
*.sqlite

# IDEs
.vscode/
.idea/
*.swp
*.swo

# OS
.DS_Store
Thumbs.db

# uv
uv.lock

# Logs
*.log

# Testing
.pytest_cache/
.coverage
htmlcov/
EOF
